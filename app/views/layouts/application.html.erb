<!DOCTYPE html>
<html>
  <head>
    <title>Qwiki</title>
    <%= stylesheet_link_tag "application" %>
    <%= javascript_include_tag "application" %>
    <%= csrf_meta_tag %>
  </head>
  <body>
    <%= render :partial => "layouts/header" %>
    <div id="container" class="<%= controller.controller_name %>">
      <p class="notice"><%= notice %></p>
      <p class="alert"><%= alert %></p>
      <%= yield %>
    </div>
  </body>
</html>
